[ -f ~/antigen.zsh ] && source ~/antigen.zsh

# Load the oh-my-zsh's library.
antigen use oh-my-zsh

# Bundles from the default repo (robbyrussell's oh-my-zsh).
# antigen bundle git
# antigen bundle heroku
# antigen bundle pip
# antigen bundle lein

# Syntax highlighting bundle.
antigen bundle zsh-users/zsh-syntax-highlighting
antigen bundle zsh-users/zsh-autosuggestions
antigen bundle zsh-users/you-should-use

# Load the theme.
# antigen theme agnoster

# generate Antigen's cache with currently loaded Bundles
antigen cache-gen

# Makes use of caching in order to quickly load bundles
# antigen init ~/.zsh_antigenrc

# Tell Antigen that you're done.
antigen apply
